{
    "name": "WordPress Dependencies",
    "homepage": "https://newclarity.github.io/wordpress-dependencies",
    "providers": true,
    "output-dir": ".",
    "output-html": true,
    "archive": {
        "directory": "downloads",
        "absolute-directory": "downloads",
        "format": "zip",
        "prefix-url": "https://newclarity.github.io/wordpress-dependencies"
    },
    "repositories": [
        {
            "type": "artifact",
            "url": "artifacts/"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "git",
            "url": "https://github.com/composer/installers"
        },
        {
            "type": "git",
            "url": "https://github.com/johnpbloch/wordpress-core"
        },
        {
            "type": "git",
            "url": "https://github.com/johnpbloch/wordpress-core-installer"
        },
        {
            "type": "git",
            "url": "https://github.com/newclarity/nprapi-wordpress.git"
        },
        {
            "type": "git",
            "url": "https://github.com/wplib/wplib.git"
        },
        {
            "type": "git",
            "url": "https://github.com/algolia/algoliasearch-wordpress.git"
        }
    ],
